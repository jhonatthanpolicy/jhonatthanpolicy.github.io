{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": "K-means clusters of countries by fertility rate (2022)",
  "data": {
    "url": "https://raw.githubusercontent.com/jhonatthanpolicy/jhonatthanpolicy.github.io/refs/heads/main/Data/cc10_fertility_clusters_2022.csv",
    "format": { "type": "csv" }
  },
  "mark": { "type": "point", "filled": true, "size": 80 },
  "encoding": {
    "x": { "field": "fertility", "type": "quantitative", "title": "Fertility rate" },
    "y": { "field": "cluster", "type": "ordinal", "title": "Cluster" },
    "tooltip": [
      { "field": "country", "type": "nominal" },
      { "field": "fertility", "type": "quantitative", "format": ".2f" },
      { "field": "cluster", "type": "ordinal" }
    ]
  }
}

