{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "title": {
    "text": "Population",
    "anchor": "start",
    "subtitle": "Total | Source: JSB (WB) through ECO API",
    "fontSize": "14",
    "subtitleFontSize": "12",
    "color": "#000000",
    "subtitleColor": "#000000"
  },
  "height": 250,
  "width": "container",
  "autosize": { "type": "fit", "contains": "padding" },
  "background": "#ffffff",
  "view": { "fill": "#ffffff", "strokeOpacity": 0 },
  "data": {
    "url": "https://api.economicsobservatory.com/jpn/popu?vega",
    "format": { "type": "json", "property": "data" }
  },
  "mark": { "type": "point", "color": "#f4134d", "strokeWidth": 2 },
  "encoding": {
    "x": { "field": "date", "type": "temporal", "axis": { "title": "", "grid": false } },
    "y": { "field": "value", "type": "quantitative", "axis": { "title": "", "grid": false } }
  }
}
