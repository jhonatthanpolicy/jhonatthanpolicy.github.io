{
  "$schema": "https://vega.github.io/schema/vega-lite/v6.json",
  "title": {
    "text": "Population",
    "anchor": "start",
    "subtitle": "Total, ten thousands | NBS through ECO API",
    "fontSize": 14,
    "subtitleFontSize": 12
  },
  "height": 250,
  "width": "container",
  "data": {
    "url": "https://raw.githubusercontent.com/EconomicsObservatory/api/main/cached_data/chn/popu.json",
    "format": { "type": "json" }
  },
  "mark": { "type": "line", "color": "#f4134d", "strokeWidth": 2 },
  "encoding": {
    "x": { "field": "date", "type": "temporal" },
    "y": { "field": "value", "type": "quantitative" }
  }
}
